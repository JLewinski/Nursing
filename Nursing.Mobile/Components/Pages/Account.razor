@page "/account"
@inject NavigationManager Navigation
@inject Nursing.Mobile.Services.SyncService Sync

<div class="container">
    <div class="row">
        <div class="col-12">
            <h1>Account</h1>
        </div>
    </div>
    <SyncActions />
</div>

@code {
    protected override async Task OnInitializedAsync()
    {
        if (!await Sync.IsLoggedIn())
        {
            Navigation.NavigateTo("/login");
        }
    }


}
